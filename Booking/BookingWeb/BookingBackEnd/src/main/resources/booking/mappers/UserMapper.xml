<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="booking.mappers.UserMapper">

	 <insert id="createNewUser" parameterType="com.booking.common.entity.User" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
	  	INSERT INTO users(email,enabled,first_name,last_name,password,photos) 
	  	VALUES (#{email},#{enabled},#{firstName},#{lastName},#{password}, default)
	 </insert>


</mapper>